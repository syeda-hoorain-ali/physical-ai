{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "fastapi",
  "functions": {
    "backend/src/main.py": {
      "runtime": "@vercel/python@4.3.1",
      "excludeFiles": "book-source/** specs/** history/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/src/main.py"
    }
  ]
}