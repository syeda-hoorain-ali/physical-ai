{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "ignoreCommand": "echo 'Ignoring extra directories'",
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "['../book source/**', '../specs/**', '../history/**']"
    },
    "./**/*": {
      "excludeFiles": "['../book source/**', '../specs/**', '../history/**']"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.py"
    }
  ]
}